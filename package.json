{
  "name": "react-template",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite dev",
    "start": "vite preview --host 0.0.0.0",
    "prebuild": "npm run generate-pwa-assets",
    "build": "vite build",
    "build:cloudflare": "node scripts/cloudflare-build.js",
    "serve": "vite preview",
    "test": "vitest run",
    "lint": "eslint",
    "format": "prettier",
    "check": "prettier --write . && eslint --fix",
    "generate-icons": "node scripts/generate-icons.js",
    "generate-manifest": "node scripts/generate-manifest.js",
    "generate-pwa-assets": "node scripts/generate-manifest.js && echo 'PWA assets generated'"
  },
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/node": "^16.11.47",
    "@types/react": "^17.0.48",
    "@types/react-dom": "^17.0.17",
    "@types/uuid": "^8.3.4",
    "@vitejs/plugin-react": "^2.0.0",
    "typescript": "^4.7.4",
    "vite": "^3.0.4"
  }
}